{
  "include": ["./gulpfile.mjs"],
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "declaration": true,
    "declarationMap": true,
    "emitDeclarationOnly": true,
    "module": "es2022",
    "lib": ["es2022"],
    "moduleResolution": "nodenext",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strict": true,
    "target": "es2022"
  }
}
